{
  "name": "advanced-web-editor",
  "main": "./lib/advanced-web-editor",
  "version": "0.1.9",
  "description": "A markdown website editing tool using Git as an underlying tool for publishing",
  "keywords": [
    "web",
    "editor",
    "git"
  ],
  "activationCommands": {
    "atom-workspace": "advanced-web-editor:toggle"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "repository": "https://github.com/ExentriqLtd/Advanced-Web-Editor",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "async-lock": "^1.1.0",
    "cson": "^4.1.0",
    "get-folder-size": "^1.0.0",
    "git-promise": "^0.3.1",
    "moment": "^2.18.1",
    "q": "^1.5.0",
    "request": "^2.81.0",
    "rimraf": "^2.6.2"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  }
}
